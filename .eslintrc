{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "quotes": [2, "single"],
    "eol-last": [0],
    "no-mixed-requires": [0],
    "no-underscore-dangle": [0],
    "global-strict": [2, "always"],
    "strict": 0
  },
  "globals": {
    "__DEV__": true
  }
}
